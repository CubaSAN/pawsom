{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "indent": [2, 2, {"SwitchCase": 1}],
      "constructor-super": 2,
      "no-console": 0,
      "no-alert": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/prop-types": 0,
      "react/jsx-max-props-per-line": 1,
      "react/jsx-no-bind": 1
    }
}
